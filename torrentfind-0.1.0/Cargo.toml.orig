[package]
name = "torrentfind"
description = "A cli tool that finds torrents on 1337x.to"
repository = "https://gitlab.com/capnhawkbill/torrentfind"
categories = ["command-line-utilities"]
license = "GPL-3.0"
version = "0.1.0"
authors = ["Sem Lindhout"]
edition = "2018"

[dependencies]
reqwest = { version = "0.10.4", features = ["blocking"] }
atty = "0.2.14"
clap = "2.33.0"
thiserror = "1.0.16"
scraper = "0.12.0"
html-extractor = "0.4.0"
colorful = "0.2.1"
term-table = "1.3.0"
regex = "1.3.7"
